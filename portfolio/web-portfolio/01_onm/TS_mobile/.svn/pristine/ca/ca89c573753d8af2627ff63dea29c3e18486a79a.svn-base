}

.slideshow {
    width: 100%;
    overflow: hidden;
    position: relative;
}

.slideshow img {
    width: 100%;
    height: 100%;
}

.slick-prev:before {
    content: '';
}

.slick-next:before {
    content: '';
}

.slick-prev,
.slick-next {
    font-size: 20px;
    line-height: 0;
    position: absolute;
    top: 93%;
    display: block;
    width: 30px;
    height: 20px;
    padding: 0;
    -webkit-transform: translate(0, -50%);
    -ms-transform: translate(0, -50%);
    transform: translate(0, -50%);
    cursor: pointer;
    color: #fff;
    border: none;
    outline: none;
}

.slick-prev {
    left: 9%;
    z-index: 1;
    bottom: 0;
    transform: translate(0px, 0px);
}

.slick-next {
    right: 80.2%;
    bottom: 0;
    transform: translate(0px, 0px);
}

.slick-prev i,
.slick-next i {
    color: #fff
}

.slick-prev button,
.slick-next button {
    background-color: rgba(0, 0, 0, 0.644);
    border: none;
    padding: 5px 9px;
    cursor: pointer;
    font-size: 30px;
    color: #999;
}

