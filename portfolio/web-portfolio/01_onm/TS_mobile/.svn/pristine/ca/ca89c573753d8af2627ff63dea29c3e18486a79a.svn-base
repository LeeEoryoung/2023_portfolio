/*========= [ end :: 공통 css ] =========*/

/*----------- [start :: header] -----------*/

header {
    background-color: #fff;
    padding: 10px 0;
}

.header_inner {
    overflow: hidden;
    margin: auto;
}

.header_inner .logo {
    float: left;
    margin-top: 10px;
}

.header_inner .gnb {
    float: right;
}

.lnb {
    float: right;
    margin-top: 5px;
}

.trigger {
    width: 30px;
    height: 18px;
    display: inline-block;
    position: relative;
    cursor: pointer;
    transform: translateY(5px);
    margin-top: 5px;
}

.trigger span {
    width: 100%;
    height: 2px;
    position: absolute;
    transition: 0.5s;
    background-color: #999;
}

.trigger span:nth-child(1) {
    top: 0;
}

.trigger span:nth-child(2) {
    top: 50%;
}

.trigger span:nth-child(3) {
    top: 100%;
}

.trigger.active span:nth-child(1) {
    top: 50%;
    transform: rotate(45deg);
}

.trigger.active span:nth-child(2) {
    display: none;
}

.trigger.active span:nth-child(3) {
    top: 50%;
    transform: rotate(-45deg);
}

.gnb {
    position: absolute;
    background-color: #fff;
    height: 500px;
    top: 50px;
    right: -380px;
    width: 375px;
    transition: 0.5s;
}

.gnb.active {
    right: 0;
}

.dropdown-navi {
    cursor: pointer;
    font-size: 20px;
    line-height: 2.5em;
    padding-left: 20px;
}

.dropdown-navi i {
    float: right;
    margin-top: 14px;
    margin-right: 20px;
    transition: 0.5s;
}

.dropdown-navi.active i {
    transform: rotate(180deg);
}

.dropdown-navi.active {
    background-color: #000;
    color: #fff;
}

.dropdown-navi:nth-child(1) {
    margin-top: 30px;
}

.dropdown-navi:last-child {
    font-size: 17.5px;
}

.dropdown-content {
    display: none;
}

.dropdown-content a {
    border-bottom: 1px solid #ccc;
    display: block;
    left: 2em;
    background-color: #fff;
    color: #000;
    text-transform: uppercase;
    padding: 10px;
    padding-left: 20px;
    width: 100%;
    font-size: 13px;
}

/*=============== [end :: header] ==============*/

