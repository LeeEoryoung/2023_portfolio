/* default css */
* {
    box-sizing: border-box;
    outline: none;
}

html,
body {
    width: 100%;
    border: 0;
    margin: 0;
    max-width: 720px;
    background-color: lightgray;
}

body {
    font-family: 'Noto Sans KR', sans-serif;
