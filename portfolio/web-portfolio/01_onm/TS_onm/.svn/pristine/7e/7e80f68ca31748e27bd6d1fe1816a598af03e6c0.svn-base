/*************************** 공동구매 css ***********************/
.gb_banner {
    position: relative;
}

.gb_heading {
    position: absolute;
    width: 1140px;
    height: 240px;
    top: 0;
    left: 16%;
}

.gb_banner .gb_heading h1 {
    font-size: 40px;
    font-weight: 700;
    color: #fff;
    padding: 40px 0 20px 20px;
}

.gb_banner .gb_heading p {
    font-weight: 500;
    font-size: 16px;
    color: #fff;
    padding-left: 20px;
}

.gb_inner {
    width: 1140px;
    margin: auto;
    background-color: #fff;
}
.gb_inner .gb_top_btn {
    overflow: hidden;
    margin-top: 30px;
    margin-bottom: 30px;
}
.gb_inner .gb_top_btn button {
    background-color: #fff;
    border: 1px solid #0ca950;
    width: 110px;
    height: 40px;
    margin: 0;
    font-size: 16px;
    font-weight: 700;
    color: #0ca950;
    cursor: pointer;
    text-align: center;
    float: left;
    transition: 0.3s;
}
.gb_top_btn button:first-child {
    border-radius: 5px 0 0 5px;
}
.gb_top_btn button:last-child {
    border-radius: 0 5px 5px 0;
}
.gb_top_btn button:hover {
    color: #fff;
    background-color: #00c950;
}
.gb_inner h3 {
    font-size: 26px;
    font-weight: 700;
    margin: 30px 0;
}

.gb_inner .gb_items_top {
    display: flex;
    margin-bottom: 40px;
    gap: 25px;
}

.gb_inner .gb_items_bt {
    display: flex;
    gap: 25px;
}

.gb_items_top .gb_item,
.gb_items_bt .gb_item {
    flex: 1;
    width: 266px;
    border-radius: 3px;
    border: 1px solid #e1e1e1;
}

.gb_items_top .gb_item {
    margin-bottom: 40px;
}

.gb_item {
    position: relative;
}
.gb_up {
    height: 162px;
    background-repeat: no-repeat;
}

.gb_down {
    padding: 20px;
}

.gb_items_top .gb_item:nth-child(1) .gb_up {
    background-image: url(images/gb_items01.jpg);
}

.gb_items_top .gb_item:nth-child(2) .gb_up {
    background-image: url(images/gb_items02.jpg);
}

.gb_items_top .gb_item:nth-child(3) .gb_up {
    background-image: url(images/gb_items03.jpg);
}
.gb_items_top .gb_item:nth-child(4) .gb_up {
    background-image: url(images/gb_items04.jpg);
}

.gb_items_bt .gb_item:nth-child(1) .gb_up {
    background-image: url(images/gb_items05.jpg);
}

.gb_items_bt .gb_item:nth-child(2) .gb_up {
    background-image: url(images/gb_items06.jpg);
}

.gb_items_bt .gb_item:nth-child(3) .gb_up {
    background-image: url(images/gb_items07.jpg);
}
.gb_items_bt .gb_item:nth-child(4) .gb_up {
    background-image: url(images/gb_items08.jpg);
}
.gb_down small {
    font-weight: 500;
    font-size: 14px;
    color: #666;
    padding-bottom: 10px;
}

.gb_down small>span {
    font-weight: 700;
    font-size: 22px;
    color: #014282;
    padding: 10px;
}

.gb_down p {
    font-weight: 400;
    font-size: 16px;
    padding-bottom: 10px;
}

.gb_down strong {
    font-weight: 500;
    font-size: 16px;
    color: #00c950;
}

.gb_down .gb_sp {
    font-weight: 700;
    font-size: 14px;
    color: #666;
    padding-top: 10px;
}

.gb_down .gb_sp>span {
    font-weight: 700;
    font-size: 26px;
    color: #ff3600;
}

.gb_inner .see_more {
    margin-top: 50px;
    margin-bottom: 140px;
    font-weight: 700;
    font-size: 16px;
    color: #014282;
    background-color: #fff;
    border: 1px solid #014282;
    border-radius: 3px;
    width: 140px;
    height: 40px;
    margin-left: 45%;
    cursor: pointer;
}
