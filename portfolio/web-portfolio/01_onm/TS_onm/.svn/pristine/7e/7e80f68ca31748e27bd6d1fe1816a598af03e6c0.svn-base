}

/* /esm banner css */



/****** [start] member common style *******/

.join, .login, .my_page, .id_find, .pw_find{
    width: 560px;
    margin: auto;
    margin-bottom: 50px;
}

.join>h1, .login>h1, .my_page>h1, .id_find>h1, .pw_find>h1{
