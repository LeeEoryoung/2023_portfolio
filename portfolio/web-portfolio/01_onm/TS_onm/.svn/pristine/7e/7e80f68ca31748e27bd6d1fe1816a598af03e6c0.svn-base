/************************ join css **********************/

.join .join_inner {
    background-color: #fff;
    text-align: left;
    padding: 40px 50px;
    border-top: 4px solid #0ca950;
}

.join_inner p {
    font-weight: 400;
    font-size: 16px;
    margin: 0 0 20px 0;
}

.join_inner p span {
    font-weight: 700;
    font-size: 20px;
}

.join_inner label {
    font-weight: 400;
    font-size: 13px;
}

.join_inner label strong {
    color: red;
}

.join_inner input[type=text],
.join_inner input[type=password],
.join_inner input[type=tel],
.join_inner input[type=file] {
    display: block;
    padding: 10px;
    width: 100%;
    margin: 7px 0;
}

/****************** input type file css *****************/
.filebox span {
    font-weight: 400;
    font-size: 13px;
    display: block;
}

.filebox .upload-name {
    display: inline-block;
    height: 40px;
    padding: 0 10px;
    margin: 5px 0;
    vertical-align: middle;
    border: 1px solid #888;
    width: 308px;
    color: #999999;
    border-radius: 3px;
}

.filebox label {
    display: inline-block;
    padding: 10px 20px;
    color: #333;
    vertical-align: middle;
    background-color: #e9ecef;
    cursor: pointer;
    height: 40px;
}

.filebox input[type="file"] {
    position: absolute;
    width: 0;
    height: 0;
    padding: 0;
    overflow: hidden;
    border: 0;
    opacity: 0;
}

/* /input file type css */
.join_inner small {
    display: block;
    font-weight: 400;
    font-size: 14px;
    color: #ff3600;
    padding: 0 0 10px 0;
}

.join_inner .join_ckbox_01 label,
.join_inner .join_ckbox_02 label {
    color: #666;
}

.join_inner .join_ckbox_01,
.join_inner .join_ckbox_02 {
    padding-top: 10px;
}

.join_inner .join_ckbox_01 label a,
.join_inner .join_ckbox_02 label a {
    text-decoration: underline;
}

.join_inner button {
    background-color: #014282;
    border: none;
    padding: 13px 30px;
    color: #fff;
    margin-top: 15px;
    width: 100%;
    border-radius: 5px;
    cursor: pointer;
    font-weight: 700;
    font-size: 16px;
}

/* /join css */
