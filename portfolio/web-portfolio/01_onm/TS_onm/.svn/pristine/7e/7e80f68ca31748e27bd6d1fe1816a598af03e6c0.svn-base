/* google web font */
@import url('https://fonts.googleapis.com/css2?family=Noto+Sans+KR:wght@400;500;700&display=swap');

/* Font Awesome 4.7.0 CND */
@import url('https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css');

/* default css */
* {
    box-sizing: border-box;
}

html,
body {
    width: 100%;
    border: 0;
    margin: 0;
    min-width: 1140px;
}

body {
    font-family: 'Noto Sans KR', sans-serif;
    margin: 0;
    line-height: 1.6em;
    font-size: 15px;
    color: #333;
    /* background-color: lightgray; */
}

a {
    text-decoration: none;
    color: #333;

}

.container {
    width: 100%;
}

.container.member {
    background-color: #f8f8f8;
}

/*********************************** header css **************************/
.contents {
    /* width: 1140px; */
}

.header_inner {
    width: 100%;
    height: 80px;
}

header {
    width: 100%;
    /* border-bottom: 1px solid #bbb; */
    background-color: #fff;
    position: relative;
    z-index: 2;
}

nav {
    height: 80px;
    width: 1140px;
    margin: auto;
    /* position: relative; */
    display: flex;
}

.header_inner .logo {
    float: left;
    margin-top: 23px;
}

.navi {
    width: 740px;
}

.gnb {
    list-style: none;
    height: 80px;
    margin-top: 3px;
    font-size: 17px;
}

.gnb ul {
    float: right;
}

.gnb li {
    float: left;
    height: 50px;
    text-align: center;
    padding: 22px 40px;
    position: relative;
    margin-bottom: 0;
    font-weight: 700;
}

.gnb li>a {
    margin-top: 27px;
    margin-right: 50px;
    color: #333;
    display: block;
    height: 100%;
    margin: 0;
}

.sub_inner {
    width: 100%;
    height: 110px;
    position: absolute;
    top: 80px;
    left: 0px;
    background-color: #f8f8f8;
    border-bottom: 1px solid #d6d6d6;
    display: block;
    transition: margin-top .3s ease-in-out;
    -webkit-transition: margin-top .3s ease-in-out;
    margin-top:-112px;
    z-index:-9;
}
.sub_bg {
    width: 100%;
    height: 80px;
    position: absolute;
    top: 0px;
    left: 0px;
    background-color: white;
    border-bottom: 1px solid #d6d6d6;
    display: block;
    z-index:-1;
}

.navi:hover .sub_inner {
    transition: margin-top .4s ease-in-out;
    -webkit-transition: margin-top .4s ease-in-out;
    margin-top:0;
}

.sub-menu {
    width: 1140px;
    margin: auto;
    display: flex;
}

.sub-menu ul {
    display: inline-block;
    list-style: none;
    padding: 0;
}

.sub-menu ul.item1 {
    margin-left: 314px;
}

.sub-menu ul.item2 {
    margin-left: 30px;
}

.sub-menu ul.item3 {
    margin-left: 44px;
}

.sub-menu a {
    padding: 5px;
    display: block;
    transition: 0.5s;
    color: #333;
    font-size: 16px;
    font-weight: 500;
    text-align: center;
}

.sub-menu a:hover {
    background-color: #f8f8f8;
    color: #0ca950;
}

.header_login {
    float: right;
    margin-top: 25px;
    z-index: 2;
}

.header_login a {
    font-size: 15px;
    font-weight: 500;
    color: #555;
}

.header_login a:last-child::before {
    content: '　|　';
}

/* /header css */
/*************** slick css ********************************/
.slick-prev:before {
    content: '';
}

.slick-next:before {
    content: '';
}

.slick-prev,
.slick-next {
    font-size: 20px;
    line-height: 0;
    position: absolute;
    top: 74%;
    display: block;
    width: 30px;
    height: 20px;
    padding: 0;
    -webkit-transform: translate(0, -50%);
    -ms-transform: translate(0, -50%);
    transform: translate(0, -50%);
    cursor: pointer;
    color: #fff;
    border: none;
    outline: none;
}

.slick-prev {
    left: 18%;
    z-index: 1;
    bottom: 0;
    transform: translate(0px, 0px);
}

.slick-next {
    right: 78.2%;
    bottom: 0;
    transform: translate(0px, 0px);
}

.slick-prev i,
.slick-next i {
    color: #fff
}

.slick-prev button,
.slick-next button {
    background-color: rgba(0, 0, 0, 0.774);
    border: none;
    padding: 3px 10px;
    cursor: pointer;
}

/* /slick css */
/********************************* welcome section css *****************/
.welcome {
    position: relative;
    z-index: 1;
}

.welcome .heading {
    position: absolute;
    top: 0;
    left: 16%;
    text-align: left;
    padding-left: 20px;
}

.welcome .heading h1 {
    font-size: 40px;
    font-weight: 700;
    color: #fff;
    line-height: 1.5em;
}

.welcome .heading p {
    font-size: 16px;
    line-height: 1.5em;
    color: #fff;
}

.slideshow {
    width: 100%;
    height: 400px;
    overflow: hidden;
    position: relative;
}

.slideshow img {
    width: 100%;
    height: 100%;
}

/* /welcome section css */
/*********************** esm banner css ************************/

.esm_banner {
    position: relative;
}

.esm_banner .esm_heading {
    position: absolute;
    width: 1140px;
    height: 240px;
    top: 0;
    left: 16%;
}

.esm_banner .esm_heading h1 {
    font-size: 40px;
    font-weight: 700;
    color: #fff;
    padding: 40px 0 20px 20px;
}

.esm_banner .esm_heading p {
    font-weight: 500;
    font-size: 16px;
