/* google web font */
@import url('https://fonts.googleapis.com/css2?family=Noto+Sans+KR:wght@400;500;700&display=swap');

/* Font Awesome 4.7.0 CND */
@import url('https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css');

/* default css */
* {
    box-sizing: border-box;
}
html, body {width:100%; border:0; margin:0;min-width: 1140px;}

body {
    font-family: 'Noto Sans KR', sans-serif;
    margin: 0;
    line-height: 1.6em;
    font-size: 15px;
    color: #333;
    background-color: lightgray;
}

a {
    text-decoration: none;
    color: #333;

}

.container {width: 100%;}

/* header css */
.contents{
    /* width: 1140px; */
}

.header_inner{
    width: 100%;
    height: 80px;
}

header {
    width: 100%;
    border: 1px solid #bbb;
    background-color: #fff;
    position: relative;
}

nav {
    height: 80px;
    width: 1140px;
    margin: auto;
    /* position: relative; */
    display: flex;
}

.header_inner .logo {
    float: left;
    margin-top: 23px;
}

.navi {
    width: 740px;
}

.gnb {
    /* float: right; */
    /* padding-top: 22px; */
    list-style: none;
    height: 80px;
    /* width: 1140px; */
    /* position: absolute; */
    /* z-index: 1; */
    /* margin-left: 250px; */
    margin-top: 3px;
    font-size: 17px;
}

.gnb ul {
    float: right;
}

.gnb li {
    float: left;
    height: 50px;
    text-align: center;
    /* width: calc(calc(1270px - 50px)/7); */
    padding:22px 40px;
    position: relative;
    margin-bottom: 0;
    font-weight: 700;
}

.gnb li>a {
    margin-top: 27px;
    margin-right: 50px;
    color: #333;
    display: block;
    height: 100%;
    margin: 0;
}

.sub_inner{
    width:100%;
    height:110px;
    position:absolute;
    top: 80px;
    left: 0px;
    background-color: #f8f8f8;
    display: none;
}

.navi:hover .sub_inner{
    display: block;
}

.sub-menu {
    width: 1140px;
    margin: auto;
    display: flex;
}

.sub-menu ul{
    display: inline-block;
    list-style: none;
    padding: 0;
}

.sub-menu ul.item1{
    margin-left:314px;
}
.sub-menu ul.item2{
    margin-left:35px;
}
.sub-menu ul.item3{
    margin-left:40px;
}

.sub-menu a {
    padding: 5px;
    display: block;
    transition: 0.5s;
    color: #333;
    font-size: 16px;
    font-weight: 500;
    text-align: center;
}

.sub-menu a:hover {
    background-color: #f8f8f8;
    color: #0ca950;
}

.header_login {
    float: right;
    margin-top: 25px;
    z-index: 2;
}

.header_login a {
    font-size: 15px;
    font-weight: 500;
    color: #555;
}

.header_login a:last-child::before {
    content: '　|　';
}

/* /header css */
/* slick css */
.slick-prev:before {
    content: '';
}
.slick-next:before {
    content: '';
}
.slick-prev,
.slick-next{
    font-size: 20px;
    line-height: 0;
    position: absolute;
    top: 81%;
    display: block;
    width: 30px;
    height: 20px;
    padding: 0;
    -webkit-transform: translate(0, -50%);
    -ms-transform: translate(0, -50%);
    transform: translate(0, -50%);
    cursor: pointer;
    color: #fff;
    border: none;
    outline: none;
    
    }
    .slick-prev {
        left: 23.3%;
        z-index: 1;
        bottom: 0;
        transform: translate(0px, 0px);
    }
    .slick-next {
        right: 73%;
        bottom: 0;
        transform: translate(0px, 0px);
    }
    .slick-prev i,
    .slick-next i {
        color:#fff
    }
    .slick-prev button,
    .slick-next button {
        background-color: rgba(0, 0, 0, 0.774);
        border: none;
        padding: 3px 10px;
        cursor: pointer;
    }
/* /slick css */
/* welcome section css */
.welcome {
    position: relative;
}

.welcome .heading {
    position: absolute;
    top: 0;
    left: 23%;
    text-align: left;
}
.welcome .heading h1 {
    font-size: 40px;
    font-weight: 700;
    color: #fff;
    line-height: 1.5em;
}
.welcome .heading p {
    font-size: 16px;
    line-height: 1.5em;
    color: #fff;
}
.slideshow {
    width: 100%;
    height: 400px;
    overflow: hidden;
    position: relative;
}
.slideshow img {
    width: 100%;
    height: 100%;
}
/* /welcome section css */
/* esm banner css */

.esm_banner {
    position: relative;

}

.esm_banner h1 {
    position: absolute;
    top: 50px;
    left: 17%;
    color: #fff;
}

.esm_banner p {
    position: absolute;
    top: 80px;
    left: 17%;
    padding-top: 10px;
    color: #fff;
}


/* /esm banner css */
/* join css */
.join {
    width: 500px;
    margin: auto;
    padding: 50px 0;
}

.join h1 {
    text-align: center;
    font-weight: 700;
}

.join .join_inner {
    background-color: #fff;
    text-align: left;
    padding: 50px;
    border-top: 4px solid #0ca950;
}

.join_inner p {
    font-weight: 400;
    font-size: 16px;
}

.join_inner p span {
    font-weight: 700;
    font-size: 20px;
}

.join_inner label {
    font-weight: 400;
    font-size: 13px;
}

.join_inner label strong {
    color: red;
}

.join_inner input[type=text],
.join_inner input[type=password],
.join_inner input[type=tel],
.join_inner input[type=file] {
    display: block;
    padding: 10px;
    width: 100%;
    margin: 7px 0;
}

/* input type file css */
.filebox span {
    font-weight: 400;
    font-size: 13px;
    display: block;
}

.filebox .upload-name {
    display: inline-block;
    height: 40px;
    padding: 0 10px;
    vertical-align: middle;
    border: 1px solid #888;
    width: 308px;
    color: #999999;
    border-radius: 3px;
}

.filebox label {
    display: inline-block;
    padding: 10px 20px;
    color: #333;
    vertical-align: middle;
    background-color: #e9ecef;
    cursor: pointer;
    height: 40px;
}

.filebox input[type="file"] {
    position: absolute;
    width: 0;
    height: 0;
    padding: 0;
    overflow: hidden;
    border: 0;
    opacity: 0;
}
/* /input file type css */
.join_inner small {
    display: block;
    font-weight: 400;
    font-size: 14px;
    color: #ff3600;
    padding-top: 5px;
}

.join_inner .join_ckbox_01 label,
.join_inner .join_ckbox_02 label {
    margin: 5px;
    color: #666;
}

.join_inner .join_ckbox_01,
.join_inner .join_ckbox_02 {
    padding-top: 10px;
}

.join_inner .join_ckbox_01 label span,
.join_inner .join_ckbox_02 label span {
    border-bottom: 1px solid #666;
}

.join_inner button {
    background-color: #014282;
    border: none;
    padding: 13px 30px;
    color: #fff;
    margin-top: 15px;
    width: 100%;
    border-radius: 5px;
    cursor: pointer;
    font-weight: 700;
    font-size: 16px;
}
/* /join css */
/* my page css */
.my_page {
    width: 500px;
    margin: auto;
    padding: 50px 0;
}

.my_page h1 {
    text-align: center;
    font-weight: 700;
}

.my_page .my_page_inner {
    background-color: #fff;
    text-align: left;
    padding: 50px;
    border-top: 4px solid #0ca950;
}

.my_page_inner label {
    font-weight: 400;
    font-size: 13px;
}

.my_page_inner label strong {
    color: red;
}
.my_page_inner .ro {
    background-color: rgba(136, 136, 136, 0.363);
    border: lightgray;
}
.my_page_inner input[type=text],
.my_page_inner input[type=password],
.my_page_inner input[type=tel],
.my_page_inner input[type=file] {
    display: block;
    padding: 10px;
    width: 100%;
    margin: 7px 0;
}

.my_page_inner small {
    display: block;
    font-weight: 400;
    font-size: 14px;
    color: #ff3600;
    padding-top: 5px;
}

.my_page_inner button {
    background-color: #014282;
    border: none;
    padding: 13px 30px;
    color: #fff;
    margin-top: 15px;
    width: 100%;
    border-radius: 5px;
    cursor: pointer;
    font-weight: 700;
    font-size: 16px;
}

/* /my page css */
/* login css */
.login {
    width: 560px;
    margin: auto;
    margin-bottom: 50px;
}

.login h1 {
    text-align: center;
    font-weight: 700;
    font-size: 40px;
    color: #333;
    margin-bottom: 40px;
}

.login_inner {
    background-color: #fff;
    padding: 30px 50px 40px 50px;
    border-top: 4px solid #0ca950;
}

.login_inner .login_inputs {
    width: 400px;
    margin: auto;
}

.login_inner .login_inputs label {
    font-size: 14px;
    font-weight: 500;
    color: #666;
}

.login_inner .login_inputs label:nth-child(6) {
    font-size: 16px;
    font-weight: 400;
    color: #666;
}

.login_inner .login_inputs input[type=text],
.login_inner .login_inputs input[type=password] {
    display: block;
    background-color: #fafafa;
    border: 1px solid #333;
    border-radius: 5px;
    width: 400px;
    height: 56px;
    margin-bottom: 30px;
    margin-top: 20px;
    color: #999;
    font-weight: 400;
    font-size: 16px;
}

.login_inner .login_inputs input[type=checkbox] {
    margin: 0 10px 30px 0;
}

.login_inner button {
    background-color: #014282;
    border: none;
    padding: 13px 30px;
    color: #fff;
    margin-top: 15px;
    width: 400px;
    height: 56px;
    border-radius: 5px;
    cursor: pointer;
    font-weight: 700;
    font-size: 16px;
    display: block;
    margin-bottom: 30px;
}

.login_bt_a {
    text-align: center;
}

.login_bt_a a {
    color: #0ca950;
    font-weight: 500;
    font-size: 16px;
}

.login_bt_a a:last-child::before {
    content: '　|　';
    color: #888;
}

/* /login css */
/* login modal */
.id_wn_pop {
    background-color: #fff;
    width: 380px;
    border: 4px solid #0ca950;
    padding: 30px;
    text-align: center;
    margin: auto;
    margin-bottom: 50px;
    margin-top: 50px;
}

.id_wn_pop p {
    font-weight: 400;
    font-size: 16px;
    color: #333;
}

.id_wn_pop button {
    width: 200px;
    height: 45px;
    background-color: #014282;
    font-weight: 700;
    font-size: 16px;
    color: #fff;
    border: none;
    border-radius: 5px;
    cursor: pointer;
}
/* /login modal */
/* id find ~ pw find css */
.contents {
    overflow: hidden;
}
.contents h1 {
    font-weight: 700;
    font-size: 30px;
    color: #333;
}
.id_find {
    float: left;
    padding-left: 24%;
    padding-top: 20px;
}
.pw_find {
    float: right;
    padding-right: 24%;
    padding-top: 20px;
    padding-bottom: 40px;
}
.id_find h1,
.pw_find h1{
    text-align: center;
}
.id_find_inner,
.pw_find_inner {
    width: 400px;
    background-color: #fff;
    padding: 30px;
    border-top: 4px solid #00c950;
}

.id_find_inner p span,
.pw_find_inner p span {
    font-weight: 700;
    font-size: 18px;
}

.id_find_input input[type=text],
.id_find_input input[type=email],
.pw_find_input input[type=text],
.pw_find_input input[type=email],
.id_find_input button,
.pw_find_input button {
    display: block;
    width: 100%;
    height: 50px;
    margin-top: 10px;
    margin-bottom: 20px;
}
.id_find_input button,
.pw_find_input button {
    background-color: #014282;
    border: none;
    color: #fff;
    border-radius: 5px;
    cursor: pointer;
    font-weight: 500;
    font-size: 16px;
}
/* /id find ~ pw find */
/* 공동구매 신청 css */
.group_pur_app_inner {
    width: 520px;
    background-color: #fff;
    margin: auto;
    padding: 30px;
    border-top: 2px solid #0ca950;
    margin-top: 50px;
    margin-bottom: 50px;
    position: relative;
}
.group_pur_app_inner h1 {
    text-align: center;
    font-size: 40px;
    font-weight: 700;
    margin-bottom: 30px;
}
.group_pur_app_inner p,
.group_pur_app_inner span {
    font-size: 26px;
    font-weight: 700;
    line-height: 1.3em;
    margin-top: 40px;
}
.group_pur_app_inner span {
    color: #0ca950;
}
.group_pur_app_inner label {
    font-size: 16px;
    font-weight: 500;
    color: #666;
}
.group_pur_app_inner label:nth-child(2) {
    /* border: 1px solid #000; */
    font-size: 16px;
    margin-left: 5px;
    color: #666;
    font-weight: 700;
}
.group_pur_app_inner label:nth-child(2) strong {
    font-weight: 700;
    margin-left: 25px;
    font-size: 16px;
    color: #666;
    font-weight: 700;
}
.group_pur_app_inner input[type=text] {
    height: 56px;
    width: 310px;
    font-size: 26px;
    margin: 20px 0;
    font-weight: 700;  
    margin-bottom: 30px;
}

.group_pur_app_inner button {
    display: block;
    width: 100%;
    height: 40px;
    margin-top: 20px;
    background-color: #014282;
    color: #fff;
    border: none;
    border-radius: 5px;
    cursor: pointer;
    font-size: 16px;
    font-weight: 500;
}
/* /공동구매 신청 css */
/* footer css */
footer {
    background-color: #333;
    color: #fff;
}

.footer_inner {
    width: 1140px;
    height: 233px;
    margin: auto;
    padding-top: 33px;
}

.footer_left {
    float: left;
}

.footer_left h3 {
    font-weight: 700;
    font-size: 16px;
    color: #a5a5a5;
    padding-bottom: 20px;
    margin-bottom: 0;
}

.footer_left span {
    font-weight: 700;
    font-size: 26px;
    color: #00c756;
    padding-bottom: 20px;
}

.footer_left p {
    font-size: 16px;
    color: #fff;
    font-weight: 400;
}

.footer_right {
    float: right;
    padding-left: 20px;
    margin-top: 20px;
    border-left: 1px solid #555;
}

.footer_right p {
    font-size: 12px;
    font-weight: 400;
    margin-top: 0;
}

.footer_right a {
    color: #fff;
    border-left: 1px solid #fff;
    padding: 0 7px;
    height: 3px;
    font-size: 14px;
    font-weight: 700;
}

.footer_right a:nth-child(2) {
    border-left: none;
}

/* /footer css */