/* LOGIN */
body.login {
  background: #454545;
}

.login_total_wrap {
  padding-top: 90px;
}

.login_total_wrap h1 {
  color: #03090a;
  font-size: 24px;
  text-align: center;
  padding-bottom: 10px;
}

.login_inner_wrap {
  width: 320px;
  margin: 0 auto;
  background: #fff;
}

.login_inner_wrap h2 {
  height: 52px;
  line-height: 52px;
  color: #5b5b5b;
  font-size: 16px;
  font-weight: 400;
  text-align: center;
  border-bottom: 1px solid #ddd;
}

.login_inner_wrap h2 strong {
  color: #4c5264;
}

.login_wrap {
  padding: 20px 15px;
}

.login_input {
  position: relative;
  width: 100%;
  margin-bottom: 12px;
}

.login_input input {
  display: inline-block;
  width: 100%;
  height: 32px;
  padding: 0 30px 0 5px;
}

.login_input i {
  position: absolute;
  top: 10px;
  right: 8px;
  color: #acacac;
  font-size: 0.75rem;
}

/* POPUP */
.pop_total_wrap {
  position: relative;
  background-color: #fff;
}

.pop_total_wrap h2 {
  color: #5b5b5b;
  font-size: 16px;
  font-family: 'Open Sans', 'Noto Sans KR', 'Nanum Gothic', 'NanumGothic', 'Dotum';
  font-weight: 300;
  text-align: center;
  padding: 10px 0;
  border-bottom: 1px solid #ddd;
}

.pop_total_wrap h2 strong {
  color: #4c5264;
  font-weight: 400;
}

.pop_total_wrap>p {
  color: #5b5b5b;
  font-size: 13px;
  padding-top: 20px;
}

.pop_total_wrap.attendance h4 {
  text-align: left;
}

.pop_total_wrap.attendance h4 span {
  display: inline-block;
  color: #fff;
  font-size: 14px;
  padding: 5px 10px;
  background: #f52b51;
}

.pop_total_wrap.attendance h4.none span {
  color: #777;
  font-size: 14px;
  padding: 5px 10px;
  background: #eee;
}

.pop_total_wrap.attendance h4 small {
  font-size: 0.75rem;
  font-weight: 300;
}

.btn_pop_wrap {
  padding: 10px;
  overflow: hidden;
}

.btn_pop_wrap.divide {
  border-top: 1px solid #e0e8ec;
}

.btn_pop_wrap.fixed {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  border-top: 1px solid #e0e8ec;
  background-color: #f8fbff;
}

.btn_pop_wrap.bt1 {
  border-top: 1px dashed #dbdbdb;
}

.btn_pop_wrap>* {
  vertical-align: top;
}

.btn_pop_wrap button {
  width: inherit;
  padding: 0 10px;
  margin: 0 7px;
}

.btn_pop_wrap a {
  width: inherit;
  padding: 0 25px;
  margin-left: 7px;
}

.btn_box_inner_wrap {
  padding-top: 10px;
  margin-top: 5px;
  border-top: 1px dashed #c9c9c9;
  overflow: hidden;
}

/* 로더 */
.layer_loader_outer {
  height: auto;
  text-align: center;
}

.layer_loader_cont {
  display: inline-block;
  line-height: 25px;
  padding: 10px 15px;
  background-color: #454545;
  border-radius: 5px;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
}

.layer_loader_cont p {
  color: #fff;
  text-align: center;
  font-weight: 400;
  letter-spacing: -0.025em;
  padding-top: 5px;
  margin: 0;
}

.layer_loader_cont p.img {
  padding-top: 0;
}

/* 버튼셋 */
.btns {
  display: inline-block;
  height: 34px;
  color: #fff;
  font-size: 0.75rem;
  font-family: 'Open Sans', 'Noto Sans KR', 'Nanum Gothic', 'NanumGothic', 'Dotum';
  letter-spacing: -0.025em;
  padding: 0 15px;
  outline: 0;
