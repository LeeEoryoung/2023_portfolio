@charset "utf-8";

html,
body,
div,
span,
object,
iframe,
h1,
h2,
h3,
h4,
h5,
h6,
p,
blockquote,
pre,
abbr,
address,
cite,
code,
del,
dfn,
em,
img,
ins,
kbd,
q,
samp,
small,
strong,
sub,
sup,
var,
b,
i,
dl,
dt,
dd,
ol,
ul,
li,
fieldset,
form,
label,
legend,
table,
caption,
tbody,
tfoot,
thead,
tr,
th,
td,
article,
aside,
canvas,
details,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section,
summary,
time,
mark,
audio,
video {
  margin: 0;
  padding: 0;
}

html {
  -webkit-text-size-adjust: none;
}

body {
  font-size: 12px;
  font-family: 'Open Sans', 'Noto Sans KR', 'Nanum Gothic', 'NanumGothic', 'Dotum';
  font-weight: 300;
  background: #e5e9ec;
}

html *,
body * {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

li {
  list-style-type: none;
}

img,
fieldset,
iframe {
  border: 0;
}

img {
  max-width: 100%;
}

h1,
h2,
h3,
h4,
h5,
h6 {
  font-size: 100%;
  font-weight: 400;
}

address {
  font-style: normal;
}

b,
strong,
th {
  font-weight: 400;
}

table,
th,
td {
  border: none;
  border-collapse: collapse;
  border-spacing: 0;
}

table {
  table-layout: fixed;
}

caption {
  position: relative;
  width: 0;
  height: 0;
  line-height: 0;
  font-size: 0;
  overflow: hidden;
  visibility: hidden;
}

a {
  text-decoration: none;
  transition: 0.3s ease-out;
  -moz-transition: 0.3s ease-out;
  -webkit-transition: 0.3s ease-out;
}

input,
select,
button,
textarea {
  font-size: 12px;
  font-family: 'Open Sans', 'Noto Sans KR', 'Nanum Gothic', 'NanumGothic', 'Dotum';
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

input {}

input[type="text"],
input[type="tel"],
input[type="numeric"],
input[type="password"],
input[type="search"],
input[type="number"],
input[type="email"],
input[type="date"],
input[type="url"],
input[type="file"],
input[type="time"] {
  height: 24px;
  font-weight: 300;
  padding: 0 5px;
  border: 1px solid #ddd;
}

input[type="file"] {
  padding: 0;
}

input[type="number"] {
  padding: 0 0 0 5px;
}

input[type="time"],
input[type="date"] {
  -webkit-user-select: none;
}

input::-webkit-inner-spin-button {
  display: none;
}

select {
  height: 24px;
  vertical-align: middle;
  border: 1px solid #ddd;
}

button {
  font-weight: 300;
  border: none;
  outline: 0;
  cursor: pointer;
}

textarea {
  font-weight: 300;
  border: none;
  outline: 0;
}

textarea[required]:invalid,
select[required]:invalid,
input[required]:invalid {
  border-bottom: 1px solid rgba(255, 0, 0, 0.3);
}

textarea[required],
select[required],
input[required] {
  background: url('../img/bl_input_must_arrow.gif') left top no-repeat #fff;
}

textarea[disabled],
select[disabled],
input[disabled] {
  background-color: rgba(250, 208, 208, .5);
}

textarea:focus,
select:focus,
input:focus {
  background-color: #f0f4f8;
}

textarea[readonly],
select[readonly],
input[readonly] {
  background-color: #faf0f0;
}

::-moz-placeholder,
:-moz-placeholder,
::-webkit-input-placeholder,
:-ms-input-placeholder,
::-ms-input-placeholder {
  color: #ccc;
  font-size: 12px;
}

.clearfix {
  clear: both !important;
}

.hide {
  display: none !important;
}

.hidden_box {
  position: absolute;
  width: 0;
  height: 0;
  overflow: hidden;
  font-size: 0;
  line-height: 0;
  visibility: hidden;
  border: none;
}

.hidden_menu {
  position: absolute;
  height: 0;
  overflow: hidden;
  font-size: 0;
  line-height: 0;
  visibility: hidden;
}

.tof {
  text-overflow: ellipsis !important;
  white-space: nowrap !important;
  word-break: normal !important;
  overflow: hidden !important;
}

