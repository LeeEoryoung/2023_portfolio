            <script type="text/javascript">
                function movePage(page) {
                    // GET 방식
                    var frm = document.frmSearch;
                    frm.page.value = page;
                    frm.submit();
                }

                function openLayer(id) {
                    $.fancybox({
                        width: 800,
                        type: 'iframe',
                        href: '/account/employee/edit?employeeId=' + id
                    });
                }

                function excelDownload() {
                    $.fileDownload("/account/employee/excelDownload.act", {
                        httpMethod: "post",
                        prepareCallback: function (url) {
                            call_layer_loader("body", "처리 중입니다.");
                        },
                        successCallback: function (url) {
                            remove_layer_loader("body");
                        },
                        failCallback: function (responseHtml, url) {
                            remove_layer_loader("body");
                        }
                    });
                }
            </script>
            <section class="right_wing_contents" id="right_wing_contents">
                <!-- //LIST -->
                <h3>항목 관리</h3>
                <section class="box_wrap">
                    <section class="sec_list_group">
                        <section class="box_wrap search">
                            <article class="art_search">
                                <form id="frmSearch" name="frmSearch" onSubmit="movePage(1); return false;"
                                    action="/account/employee/list" method="get">
                                    <input id="page" name="page" type="hidden" value="1" />
                                    <input id="recordSize" name="recordSize" type="hidden" value="15" />
                                    <fieldset>
                                        <legend class="hidden_box">항목관리</legend>
                                        <p class="search_line">
                                            <em>
                                                <span class="line_inner">
                                                    <select id="authority" name="authority" style="margin-right: 7px;">
                                                        <option value="">항목구분</option>
                                                        <option value="1">항목</option>
                                                        <option value="2">담보</option>
                                                    </select>
                                                </span>
                                                <span class="line_inner">
                                                    <select id="authority" name="authority">
                                                        <option value="">사용여부</option>
                                                        <option value="1">사용</option>
                                                        <option value="2">미사용</option>
                                                    </select>
                                                </span>
                                            </em>
                                            <em>
                                                <span class="line_inner">
                                                    <label>
                                                        항목명
                                                        <span>
                                                            <input id="searchKeyword" name="searchKeyword"
                                                                style="width: 150px; margin-left: 8px;" placeholder="검색"
                                                                type="text" value="" maxlength="20" />
                                                        </span>
                                                    </label>
                                                </span>
                                            </em>
                                        </p>
                                    </fieldset>
                                    <aside class="btn_search_aside">
                                        <button type="submit" class="btns green small"><i class="fa fa-search"></i>
                                            조회</button>
                                        <button type="button" class="btns orange small reset"
                                            onclick="$(this).parents('form')[0].reset()"><i
                                                class="fa fa-fw fa-plus"></i></i>
                                            등록</button>
                                    </aside>
                                </form>
                            </article>
                        </section>
                        <section class="box_wrap list">
                            <article class="art_reload">
                                <table class="tbl_sort_list list">
                                    <caption class="hidden_box">목록</caption>
                                    <colgroup>
                                        <col width="70" />
                                        <col width="30" />
                                        <col width="70" />
                                        <col width="70" />
                                        <col width="100" />
                                        <col width="30" />
                                    </colgroup>
                                    <thead>
                                        <tr>
                                            <th scope="col">항목구분</th>
                                            <th scope="col">코드</th>
                                            <th scope="col">항목명</th>
                                            <th scope="col">공통코드그룹</th>
                                            <th scope="col">조회쿼리내용</th>
                                            <th scope="col">사용여부</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr onclick="openLayer('1')">
                                            <td>항목</td>
                                            <td>1</td>
                                            <td>상해급수</td>
                                            <td>Wound_class</td>
                                            <td>
                                                SELECT IFNULL(MAX(b.wound_class), '0') INTO @v_return_value <br>
                                                FROM mh_customer_detail a , mh_company_job b WHERE a.job_code =
                                                .<br>job_small_class_code AND a.customer_id = @p_customer_idAND
                                                b.<br>company_code = @p_company_code LIMIT 1
                                            </td>
                                            <td>사용</td>
                                        </tr>
                                        <tr onclick="openLayer('2')">
                                            <td>항목</td>
                                            <td>2</td>
                                            <td>성별</td>
                                            <td></td>
                                            <td>
                                                SELECT CASE WHEN gender = 'M' THEN '1'<br>
                                                WHEN gender = 'F' THEN '2' ELSE '0' END<br>
                                                INTO @v_return_value FROM customer<br>
                                                WHERE customer_id = @p_customer_id LIMIT 1<br>
                                            </td>
                                            <td>사용</td>
                                        </tr>

                                        <tr onclick="openLayer('3')">
                                            <td>항목</td>
                                            <td>3</td>
                                            <td>보험연령</td>
                                            <td></td>
                                            <td>
                                                SELECT ROUND((TO_DAYS(DATE_ADD(NOW(), INTERVAL 6 MONTH)) -
                                                TO_DAYS(DATE_FORMAT<br>(birthdate, '%Y-%m-%d'))) / 365)<br>
                                                INTO @v_return_value FROM customer<br>
                                                WHERE customer_id = @p_customer_id LIMIT 1<br>
                                            </td>
                                            <td>사용</td>
                                        </tr>

                                        <tr onclick="openLayer('4')">
                                            <td>항목</td>
                                            <td>80</td>
                                            <td>뇌혈관질환관련 진단비</td>
                                            <td></td>
                                            <td>
                                                SELECT 50000000 INTO @v_return_value FROM DUAL
                                            </td>
                                            <td>사용</td>
                                        </tr>
                                </table>
                            </article>
                            <aside class='pagination'><button type='button' class='btn_arrow first disabled'
                                    title='첫 페이지'><i class='fa fa-angle-double-left'></i></button><button type='button'
                                    class='btn_arrow prev disabled' title='이전 페이지'><i
                                        class='fa fa-angle-left'></i></button><button type='button' class='page active'
                                    onclick='movePage(1)'><span>1</span></button><button type='button'
                                    onclick='movePage(2)'><span>2</span></button><button type='button'
                                    onclick='movePage(3)'><span>3</span></button><button type='button'
                                    onclick='movePage(4)'><span>4</span></button><button type='button'
                                    onclick='movePage(5)'><span>5</span></button><button type='button'
                                    class='btn_arrow next disabled' title='다음 페이지'><i
                                        class='fa fa-angle-right'></i></button><button type='button'
                                    class='btn_arrow last disabled' title='마지막 페이지'><i
                                        class='fa fa-angle-double-right'></i></button></aside>
                        </section>
                    </section>
                </section>
            </section>
        </section>
